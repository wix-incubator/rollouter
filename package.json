{
  "name": "rollouter",
  "version": "1.0.0",
  "private": false,
  "description": "Simple distributed feature flags/AB tests lib",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest",
    "build": "npx tsc",
    "release": "wnpm-release -- --no-shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wix-incubator/rollouter.git"
  },
  "author": "Volodymyr Shuhaiev <volodymyrs@wix.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix-incubator/rollouter/issues"
  },
  "homepage": "https://github.com/wix-incubator/rollouter#readme",
  "dependencies": {
    "i": "^0.3.6",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/md5": "^2.1.33",
    "@wix/wnpm-ci": "^6.1.0",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3",
    "uuid4": "^1.1.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testURL": "http://localhost:8081",
    "testRegex": "/(src|test)/.*\\.spec\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
